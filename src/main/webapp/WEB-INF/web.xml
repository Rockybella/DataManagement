<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="https://jakarta.ee/xml/ns/jakartaee" 
xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" 
id="WebApp_ID" version="5.0">
  
  
    <servlet>
        <servlet-name>AddParticipantServlet</servlet-name>
        <servlet-class>com.example.funfit.controller.AddParticipantServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddParticipantServlet</servlet-name>
        <url-pattern>/AddParticipantServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddBatchServlet</servlet-name>
        <servlet-class>com.example.funfit.controller.AddBatchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddBatchServlet</servlet-name>
        <url-pattern>/AddBatchServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ViewParticipantServlet</servlet-name>
        <servlet-class>com.example.funfit.controller.ViewParticipantServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewParticipantServlet</servlet-name>
        <url-pattern>/ViewParticipantServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ViewBatchServlet</servlet-name>
        <servlet-class>com.example.funfit.controller.ViewBatchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewBatchServlet</servlet-name>
        <url-pattern>/ViewBatchServlet</url-pattern>
    </servlet-mapping>

</web-app>

